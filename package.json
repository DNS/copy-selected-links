{
    "name": "copy-selected-links",
    "version": "0.0.0",
    "private": true,
    "repository": "https://gitlab.com/Marnes/copy-selected-links.git",
    "license": "MPL-2.0",
    "author": "Marnes <marnick.leau@gmail.com>",
    "scripts": {
        "build": "run-s clean prettier lint compile qa package bundle",
        "bundle": "bestzip dist/sources.zip .browserslistrc .editorconfig .eslintrc.yml .gitignore .node-version .prettierrc.yml license.md package.json pnpm-lock.yaml readme.md src tsconfig.json web-ext-config.js",
        "clean": "rimraf dist extension .cache",
        "compile": "parcel build src/*.* --out-dir extension",
        "compile:watch": "parcel watch src/*.* --out-dir extension --no-autoinstall --no-hmr",
        "eslint": "eslint src/**/*.ts",
        "preinstall": "npx only-allow pnpm",
        "lint": "run-p eslint tsc",
        "package": "web-ext build",
        "prettier": "prettier --write --list-different --ignore-unknown --ignore-path .gitignore . !pnpm-lock.yaml",
        "qa": "web-ext lint",
        "start": "web-ext run",
        "start:chrome": "web-ext run -t chromium",
        "tsc": "tsc --noEmit"
    },
    "dependencies": {
        "basscss-sass": "4.0.0",
        "fast-equals": "2.0.0",
        "type-fest": "0.20.2",
        "webextension-polyfill": "0.7.0",
        "webextension-polyfill-ts": "0.24.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "4.15.0",
        "@typescript-eslint/parser": "4.15.0",
        "bestzip": "2.1.7",
        "cssnano": "4.1.10",
        "eslint": "7.19.0",
        "eslint-config-prettier": "7.2.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prettier": "3.3.1",
        "npm-run-all": "4.1.5",
        "parcel-bundler": "1.12.4",
        "parcel-plugin-web-extension": "1.6.1",
        "prettier": "2.2.1",
        "prettier-plugin-packagejson": "2.2.9",
        "rimraf": "3.0.2",
        "sass": "1.32.7",
        "typescript": "4.1.5",
        "web-ext": "5.5.0"
    }
}
