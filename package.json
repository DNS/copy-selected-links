{
    "name": "copy-selected-links",
    "version": "0.0.0",
    "private": true,
    "license": "MPL-2.0",
    "repository": "https://gitlab.com/Marnes/copy-selected-links.git",
    "author": "Marnes <marnick.leau@gmail.com>",
    "scripts": {
        "build": "run-s clean prettify lint compile bundle review",
        "bundle": "web-ext build",
        "bundle:watch": "web-ext run",
        "bundle:watch:chrome": "web-ext run -t chromium",
        "clean": "rimraf dist extension .cache",
        "compile": "parcel build src/*.* --out-dir extension",
        "compile:watch": "parcel watch src/*.* --out-dir extension --no-autoinstall --no-hmr",
        "develop": "npm-run-all clean compile -plr compile:watch bundle:watch",
        "develop:chrome": "npm-run-all clean compile -plr compile:watch bundle:watch:chrome",
        "lint": "run-p lint:addon typecheck",
        "lint:addon": "eslint src/**/*.ts",
        "prettify": "run-p prettify:addon prettify:project",
        "prettify:addon": "prettier src/**/*.{html,json,scss,ts} --write",
        "prettify:project": "prettier *.{js,json,md,yml} --write",
        "review": "bestzip dist/sources.zip .browserslistrc .editorconfig .eslintrc.yml .gitignore .node-version .prettierrc.yml license.md package.json readme.md src tsconfig.json web-ext-config.js yarn.lock",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "fast-deep-equal": "3.1.3",
        "ow": "0.17.0",
        "type-fest": "0.16.0",
        "webextension-polyfill-ts": "0.20.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "4.1.0",
        "@typescript-eslint/parser": "4.1.0",
        "bestzip": "2.1.7",
        "eslint": "7.9.0",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-import": "2.22.0",
        "eslint-plugin-prettier": "3.1.4",
        "npm-run-all": "4.1.5",
        "parcel-bundler": "1.12.4",
        "parcel-plugin-web-extension": "1.6.1",
        "prettier": "2.1.1",
        "prettier-plugin-package": "1.0.0",
        "rimraf": "3.0.2",
        "sass": "1.26.10",
        "typescript": "4.0.2",
        "web-ext": "5.0.0"
    }
}
