{
    "name": "copy-selected-links",
    "version": "0.0.0",
    "private": true,
    "repository": "https://gitlab.com/Marnes/copy-selected-links.git",
    "license": "MPL-2.0",
    "author": "Marnes <marnick.leau@gmail.com>",
    "scripts": {
        "build": "run-s clean prettier lint compile package bundle",
        "bundle": "bestzip dist/sources.zip .browserslistrc .editorconfig .eslintrc.yml .gitignore .node-version .prettierrc.yml license.md package.json readme.md src tsconfig.json web-ext-config.js yarn.lock",
        "clean": "rimraf dist extension .cache",
        "compile": "parcel build src/*.* --out-dir extension",
        "compile:watch": "parcel watch src/*.* --out-dir extension --no-autoinstall --no-hmr",
        "eslint": "eslint src/**/*.ts",
        "lint": "run-p eslint tsc",
        "package": "web-ext build",
        "prettier": "prettier --write --list-different --ignore-unknown --ignore-path .gitignore .",
        "start": "web-ext run",
        "start:chrome": "web-ext run -t chromium",
        "tsc": "tsc --noEmit"
    },
    "dependencies": {
        "fast-equals": "2.0.0",
        "type-fest": "0.18.0",
        "webextension-polyfill-ts": "0.20.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "4.4.1",
        "@typescript-eslint/parser": "4.4.1",
        "bestzip": "2.1.7",
        "eslint": "7.11.0",
        "eslint-config-prettier": "6.13.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prettier": "3.1.4",
        "npm-run-all": "4.1.5",
        "parcel-bundler": "1.12.4",
        "parcel-plugin-web-extension": "1.6.1",
        "prettier": "2.1.2",
        "prettier-plugin-packagejson": "2.2.6",
        "rimraf": "3.0.2",
        "sass": "1.27.0",
        "typescript": "4.0.3",
        "web-ext": "5.3.0"
    }
}
